{
  "name": "@webundsoehne/nx-nest",
  "version": "1.0.0",
  "repository": "https://github.com/tailoredmedia/backend-nx-skeleton",
  "description": "Nest Plugin for Nx",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "author": "Backend <backend@webundsoehne.com>",
  "license": "ISC",
  "publishConfig": {
    "access": "public"
  },
  "generators": "./generators.json",
  "scripts": {
    "build": "tsup-node",
    "dev:start": "tsup-node --watch",
    "lint": "prettier --loglevel warn --write src/ && eslint --ext .ts,.js,.tsx,.jsx --fix src/",
    "lint:check": "eslint --ext .ts,.js,.tsx,.jsx src/"
  },
  "lint-staged": {
    "*.{ts,js,tsx,jsx}": [
      "prettier --loglevel warn --write",
      "eslint --fix"
    ],
    "*.{json,md}": [
      "prettier --loglevel warn --write"
    ]
  },
  "peerDependencies": {
    "nx": ">= 17 <= 20"
  },
  "dependencies": {
    "@nx/devkit": "^19.3.0",
    "@nx/js": "^19.3.0",
    "@nx/workspace": "^19.3.0",
    "ts-morph": "^23.0.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "nx": "^19.5.2"
  },
  "engines": {
    "node": ">= 18"
  }
}
