import { Controller } from '@nestjs/common'
import { MessagePattern } from '@nestjs/microservices'

import { DefaultMicroservice } from './default.service'
import { DefaultPattern, MessageQueues, MicroserviceRequest, MicroserviceResponse } from '@<%- scope %>/microservice-provider'

@Controller()
export class DefaultMicrocontroller {
  constructor (private readonly defaultMicroservice: DefaultMicroservice) {}

  @MessagePattern(DefaultPattern.DEFAULT)
  default (
    options: MicroserviceRequest<MessageQueues.DEFAULT, DefaultPattern.DEFAULT>
  ): Promise<MicroserviceResponse<MessageQueues.DEFAULT, DefaultPattern.DEFAULT>> {
    return this.defaultMicroservice.default(options)
  }
}
