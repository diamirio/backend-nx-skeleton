import { Controller } from '@nestjs/common'
import { MessagePattern } from '@nestjs/microservices'

import { DefaultMicroservice } from './default.service'
import { <%- projectNames.className %>Pattern, MessageQueues, MicroserviceRequest, MicroserviceResponse } from '@<%- scope %>/microservice-provider'

@Controller()
export class DefaultMicrocontroller {
  constructor (private readonly defaultMicroservice: DefaultMicroservice) {}

  @MessagePattern(<%- projectNames.className %>Pattern.DEFAULT)
  default (
    options: MicroserviceRequest<MessageQueues.<%- projectNames.constantName %>, <%- projectNames.className %>Pattern.DEFAULT>
  ): Promise<MicroserviceResponse<MessageQueues.<%- projectNames.constantName %>, <%- projectNames.className %>Pattern.DEFAULT>> {
    return this.defaultMicroservice.default(options)
  }
}
