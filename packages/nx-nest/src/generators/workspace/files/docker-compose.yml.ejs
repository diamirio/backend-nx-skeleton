include:
    - service-docker-compose.yml
services:
    <%= SERVICE_NAME %>:
        image: <%= DOCKER_IMAGE %>
        ports:
            - "3000:3000"
        working_dir: /opt/workspace
        volumes:
            - .:/opt/workspace
            - nx_cache:/opt/workspace/.nx
        command: npm run start
volumes:
    # container requires "self-generated" cache dir
    nx_cache:
