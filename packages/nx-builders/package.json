{
  "name": "@webundsoehne/nx-builders",
  "version": "1.0.0",
  "repository": "https://github.com/tailoredmedia/backend-nx-skeleton",
  "description": "TSC compiler for NX.",
  "author": "Backend <backend@webundsoehne.com>",
  "license": "ISC",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "schematics": "./schematics.json",
  "builders": "./builders.json",
  "scripts": {
    "build": "tsup-node",
    "postbuild": "cpy --cwd=src --parents --dot '**/*.json' ../dist/",
    "dev:start": "tsup-node --watch",
    "lint": "prettier --loglevel warn --write src/ && eslint --ext .ts,.js,.tsx,.jsx --fix src/",
    "lint:check": "eslint --ext .ts,.js,.tsx,.jsx src/",
    "lint:staged": "lint-staged",
    "docs:toc": "markdown-toc README.md --bullets='-' -i",
    "docs:jsdoc": "typedoc src/index.ts --readme none"
  },
  "lint-staged": {
    "*.{ts,js,tsx,jsx}": [
      "prettier --loglevel warn --write",
      "eslint --fix"
    ],
    "*.{json,md}": [
      "prettier --loglevel warn --write"
    ]
  },
  "peerDependencies": {
    "@nrwl/workspace": "*",
    "@nrwl/devkit": ">= 14",
    "nx": ">= 14",
    "ts-node": ">= 8.10.0",
    "ts-node-dev": ">= 1.0.0",
    "tsc-watch": ">= 4.2.0",
    "tsconfig-replace-paths": "^0.0.11",
    "typescript": ">= 3.9.0"
  },
  "dependencies": {
    "@angular-devkit/architect": "^0.1402.5",
    "@angular-devkit/core": "^14.2.5",
    "@angular-devkit/schematics": "^14.2.5",
    "@webundsoehne/deep-merge": "*",
    "@webundsoehne/nx-tools": "*",
    "delay": "^5.0.0",
    "dotenv": "^16.0.3",
    "execa": "^5.1.1",
    "fs-extra": "^10.1.0",
    "tree-kill": "^1.2.2"
  },
  "devDependencies": {
    "@types/execa": "^2.0.0",
    "@types/fs-extra": "^9.0.13",
    "@types/tsc-watch": "^4.2.0",
    "@nrwl/workspace": "^14.8.6",
    "@nrwl/devkit": "^14.8.6",
    "nx": "^14.8.6"
  },
  "engines": {
    "node": ">= 16"
  }
}
