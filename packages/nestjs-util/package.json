{
  "name": "@webundsoehne/nestjs-util",
  "version": "1.0.0",
  "description": "NestJS skeleton util library",
  "author": "Backend <backend@webundsoehne.com>",
  "repository": "https://github.com/tailoredmedia/backend-nx-skeleton",
  "license": "ISC",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "nestjs",
    "util"
  ],
  "scripts": {
    "build": "tsup-node",
    "dev:start": "tsup-node --watch",
    "lint": "prettier --loglevel warn --write src/ && eslint --ext .ts,.js,.tsx,.jsx --fix src/",
    "lint:check": "eslint --ext .ts,.js,.tsx,.jsx src/",
    "docs:toc": "markdown-toc README.md --bullets='-' -i",
    "docs:jsdoc": "typedoc src/index.ts --readme none"
  },
  "lint-staged": {
    "*.{ts,js,tsx,jsx}": [
      "prettier --loglevel warn --write",
      "eslint --fix"
    ],
    "*.{json,md}": [
      "prettier --loglevel warn --write"
    ]
  },
  "dependencies": {
    "@webundsoehne/deep-merge": "*",
    "colorette": "^2.0.20",
    "config": "^3.3.9",
    "js-yaml": "^4.1.0",
    "winston": "^3.11.0",
    "winston-transport": "^4.5.0"
  },
  "peerDependencies": {
    "@nestjs/common": ">= 8",
    "@nestjs/core": ">= 8",
    "@nestjs/graphql": ">= 8",
    "@nestjs/microservices": ">= 8",
    "@sentry/node": ">= 7",
    "class-transformer": ">= 0.4.0",
    "class-validator": ">= 0.13.0",
    "express": ">= 4",
    "fastify": ">= 3",
    "rxjs": ">= 7",
    "reflect-metadata": "^0.1.13"
  },
  "peerDependenciesMeta": {
    "fastify": {
      "optional": true
    },
    "@sentry/node": {
      "optional": true
    },
    "@nestjs/graphql": {
      "optional": true
    }
  },
  "devDependencies": {
    "@nestjs/common": "^9.4.0",
    "@nestjs/core": "^9.4.0",
    "@nestjs/graphql": "^11.0.5",
    "@sentry/node": "^7.48.0",
    "@types/config": "^3.3.0",
    "@types/express": "^4.17.3",
    "@types/winston": "^2.4.4",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "express": "^4.18.2",
    "fastify": "^4.15.0",
    "rxjs": "^7.8.1"
  },
  "engines": {
    "node": ">= 16"
  }
}
